{
    "name" : "mrzen/trovebox",
    "homepage" : "https://github.com/mrzen/trovebox-php",

    "description" : "Trovebox Client for PHP using Guzzle",

    "type" : "library",

    "keywords" : ["image","photo","trovebox","REST","API"],

    "license" : "GNU GPL v2 or later",

    "authors" : [
        {
            "name" : "Mr. Zen. Ltd",
            "homepage" : "http://mrzen.com/"
        }
    ],


    "support" : {
        "issues" : "https://github.com/mrzen/trovebox-php/issues"
    },


    "require" : {
        "php" : ">= 5.4.0",
        "guzzlehttp/guzzle" : "4.*",
        "guzzlehttp/oauth-subscriber" : "*",
        "guzzlehttp/log-subscriber" : "*",
        "guzzlehttp/guzzle-services" : "*"
    },

    "require-dev" : {
        "monolog/monolog" : "1.4.*",
        "phpunit/phpunit" : "4.*",
        "psr/log" : "~1.0"
    },

    "suggest" : {
        "monolog/monolog" : "Logging support"
    },


    "autoload" : {
        "psr-4" : {
            "Trovebox\\" : "src/"
        }
    },

    "autoload-dev" : {
        "psr-4" : {
            "Trovebox\\Tests\\" : "tests/"
        }
    }
}
    




            
